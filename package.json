{
    "name": "night-runner",
    "displayName": "Night Runner",
    "description": "A futuristic dark theme for Visual Studio Code",
    "version": "0.0.3",
    "publisher": "AfonsoVReis",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AfonsoVReis/night-runner.git"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "dark",
        "black",
        "theme",
        "night runner"
    ],
    "scripts": {
        "release": "standard-version",
        "postrelease": "git push --follow-tags origin HEAD"
    },
    "contributes": {
        "themes": [
            {
                "label": "Night Runner",
                "uiTheme": "vs-dark",
                "path": "./themes/Night Runner-color-theme.json"
            }
        ]
    },
    "devDependencies": {
        "standard-version": "^9.1.0"
    }
}
